{
    "name": "electron-dev",
    "dockerFile": "docker/Dockerfile",
    "build": {
        "args": {
            "-t": "electron"
        }
    },
    "customizations": {
        "vscode": {
            "settings": {
                "git.ignoreMissingGitWarning": true
            },
            "extensions": [
                "eamodio.gitlens",
                "MichaelCurrin.auto-commit-msg"
            ]
        }
    }
}