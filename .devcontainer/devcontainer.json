{
    "name": "electron-dev",
    "dockerComposeFile": "docker/docker-compose.yaml",
    "service": "electron-dev",
    "workspaceFolder": "/",
    "shutdownAction": "stopCompose",
    "customizations": {
        "vscode": {
            "settings": {
                "git.ignoreMissingGitWarning": true
            },
            "extensions": [
                "eamodio.gitlens",
                "MichaelCurrin.auto-commit-msg"
            ]
        }
    }
}